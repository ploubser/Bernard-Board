<div class="child">
<% for attribute in child_model.attributes -%>
  <p>
    <%= child_model.name %> <%%= f.label :<%= attribute.name %> %><br />
    <%%= f.<%= attribute.field_type %> :<%= attribute.name %> %>
  </p>
<% end -%>
  <p>
    <%%= f.hidden_field :_destroy, :class => 'delete' %>
    <%%= remove_child_link 'remove', f %>
  </p>
</div>
